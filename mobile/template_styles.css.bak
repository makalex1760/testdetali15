#abcd {
  display: block;

  height: 30px;
  /*margin: 0 auto;*/
  padding: 0 2px;
  background: #fff;
  border-radius: 6px;
  border: 1px solid rgba(0, 0, 0, 0.15);
  box-shadow: 0 1px 1px rgba(20, 20, 20, 0.2);
  cursor: pointer;
  outline: none;
  float:right;
  margin-top:7px;
  
}

#abcd li { display: block; position: relative; float: left; font-size: 1.1em; text-shadow: 1px 1px 0 #fff; border-right: 1px solid #dae0e5; }

#abcd li a {
  display: block;
  float: left;
  padding: 0 9px;
  line-height: 30px;
  /*text-decoration: none;*/
	color:#070D7D;
  -webkit-transition: all 0.2s linear;
  -moz-transition: all 0.2s linear;
  -o-transition: all 0.2s linear;
  transition: all 0.2s linear;
}


#abcd ul {
  position: absolute;
  top: 38px;
  width: 230px;
  background: #fff;
  display: none;
  margin: 0;
  padding: 7px 0;
  list-style: none;
  border-radius: 3px;
  border: 1px solid rgba(0, 0, 0, 0.2);
  box-shadow: 0 0 5px rgba(0, 0, 0, 0.2);
  max-height: 200px !important;
overflow-y: scroll;
overflow-x: hidden;
}

ul::-webkit-scrollbar {
-webkit-appearance: none;
 width: 7px; 
}

ul::-webkit-scrollbar-thumb {
 border-radius: 4px; 
background-color: rgba(0,0,0,.5);
-webkit-box-shadow: 0 0 1px rgba(255,255,255,.5);
}



/* Стрелочки */
#abcd ul:after {
  content: "";
  width: 0;
  height: 0;
  position: absolute;
  bottom: 100%;
  left: 4px;
  border-width: 0 8px 8px 8px;
  border-style: solid;
  border-color: #fff transparent; 
}

#abcd ul:before {
  content: "";
  width: 0;
  height: 0;
  position: absolute;
  bottom: 100%;
  left: 0px;
  border-width: 0 10px 10px 10px;
  border-style: solid;
  border-color: rgba(0, 0, 0, 0.1) transparent;  
}

#abcd ul li { 
  display: block; 
  width: 100%; 
  text-shadow: 1px 1px 0 #fff;
}

#abcd ul li a {
  display: block;
  width: 100%;
  padding: 6px 7px;
  line-height: 1em;
  -webkit-transition: all 0.2s linear;
  -moz-transition: all 0.2s linear;
  -o-transition: all 0.2s linear;
  transition: all 0.2s linear;
}
